{
  "name": "@empoleon/code-highlight",
  "version": "7.0.0",
  "description": "Code highlight with Empoleon theme",
  "homepage": "https://empoleon.dev/x/code-highlight/",
  "license": "MIT",
  "author": "Paul M",
  "keywords": [
    "code",
    "components",
    "frontend",
    "highlight",
    "library",
    "next",
    "nextjs",
    "react",
    "syntax",
    "syntax-highlight",
    "ui",
    "ui-kit"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    },
    "./styles.css": "./styles.css",
    "./styles.layer.css": "./styles.layer.css"
  },
  "repository": {
    "url": "https://github.com/empoleondev/empoleon.git",
    "type": "git",
    "directory": "packages/@empoleon/code-highlight"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "highlight.js": "^11.10.0"
  },
  "devDependencies": {
    "@empoleon/core": "workspace:*",
    "@empoleon/hooks": "workspace:*"
  }
}
