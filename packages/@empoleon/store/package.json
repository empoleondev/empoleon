{
  "name": "@empoleon/store",
  "version": "7.0.0",
  "description": "A library to sync external React state updates",
  "homepage": "https://empoleon.dev",
  "license": "MIT",
  "author": "Paul M",
  "keywords": [
    "frontend",
    "hooks",
    "library",
    "solidjs",
    "state"
  ],
  "sideEffects": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/empoleondev/empoleon.git",
    "type": "git",
    "directory": "packages/@empoleon/store"
  }
}
