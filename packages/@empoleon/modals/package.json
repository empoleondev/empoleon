{
  "name": "@empoleon/modals",
  "version": "8.0.0",
  "description": "Modals manager based on Empoleon components",
  "homepage": "https://empoleon.dev/x/modals/",
  "license": "MIT",
  "author": "Paul M",
  "keywords": [
    "components",
    "confirm",
    "frontend",
    "library",
    "modal",
    "next",
