    }
  },
  "repository": {
    "url": "https://github.com/empoleondev/empoleon.git",
    "type": "git",
    "directory": "packages/@empoleon/modals"
  },
  "devDependencies": {
    "@empoleon/core": "workspace:*",
    "@empoleon/hooks": "workspace:*"
  }
