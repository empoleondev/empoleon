      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/empoleondev",
    "type": "git",
    "directory": "packages/@empoleon-tests/core"
  },
  "devDependencies": {
    "@empoleon/core": "workspace:*",
    "@empoleon/hooks": "workspace:*",
    "solid-js": "1.9.7"
  }
}
