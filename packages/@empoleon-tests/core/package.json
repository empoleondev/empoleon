{
  "name": "@empoleon-tests/core",
  "version": "2.0.0",
  "description": "Core tests package for Empoleon",
  "license": "MIT",
  "author": "Paul M",
  "keywords": [
    "jest",
    "empoleon",
    "tests"
  ],
  "sideEffects": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
